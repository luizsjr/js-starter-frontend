{
  "name": "js-starter-frontend",
  "version": "1.0.0",
  "description": "JS Starter Frontend Project",
  "main": "src/index.js",
  "repository": "git@github.com:luizsjr/js-starter-frontend.git",
  "author": "luizsjr",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=dev webpack-dev-server --progress --open --mode development --hot --config webpack.config.js",
    "security-check": "nsp check",
    "prestart": "npm run security-check",
    "start": "nodemon --watch webpack.config.js --exec npm run dev",
    "prebuild": "rimraf ./dist && mkdir dist",
    "build": "webpack -p --progress --mode production --config webpack.config.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.5",
    "@fortawesome/fontawesome-free-solid": "^5.0.9",
    "bootstrap": "^4.0.0",
    "ejs": "^2.5.8",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.10.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.0.7",
    "install": "^0.10.4",
    "node-sass": "^4.7.2",
    "nodemon": "^1.17.3",
    "npm": "^5.7.1",
    "nsp": "^3.2.1",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.12",
    "webpack-dev-server": "^3.1.1"
  }
}
