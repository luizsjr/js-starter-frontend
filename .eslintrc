{
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:import/warnings"
  ],
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module"
  },
  "rules": {
    "no-console" : 1,
    "semi": [2, "always"]
  },
  "env": {
    "browser": true,
    "node": true,
    "jquery": true
  }
}
